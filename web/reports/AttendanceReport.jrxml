	<parameter name="BRANCH_ID" class="java.lang.String"/>
	<parameter name="PAYROLL_DATE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT e.employeeId AS id, CONCAT_WS(', ', setNameToUpper(e.lastname), CONCAT_WS(' ', setNameToUpper(e.firstname), setNameToUpper(e.middlename))) AS name, left(e.employmentWageEntry, 1) AS entry, 
(SELECT COUNT(*) FROM attendance a INNER JOIN salary sa ON a.salaryId = sa.id INNER JOIN employee e ON e.employeeId = sa.employeeId WHERE e.employmentWageEntry = 'monthly' AND a.policy = 'absent' AND a.salaryId = s.id) AS absences,
(SELECT SUM(lates) FROM attendance Where salaryId = s.id) AS lates, 
(SELECT SUM(undertime) FROM attendance Where salaryId = s.id) AS undertime, 
(SELECT SUM(overtime) FROM attendance Where salaryId = s.id) AS overtime, 
(SELECT SUM(nightDifferential) FROM attendance Where salaryId = s.id) AS nightDifferential, 
(SELECT ROUND(SUM(latesDeduction), 2) FROM attendance Where salaryId = s.id) AS latesDeduction, 
(SELECT ROUND(SUM(undertimeDeduction), 2) FROM attendance Where salaryId = s.id) AS undertimeDeduction, 
(SELECT ROUND(SUM(overtimePaid), 2) FROM attendance Where salaryId = s.id) AS overtimePaid, 
(SELECT ROUND(SUM(nightDifferentialPaid), 2) FROM attendance Where salaryId = s.id) AS nightDifferentialPaid, 
(SELECT ROUND(SUM(legalHolidayPaid), 2) FROM attendance Where salaryId = s.id) AS legalHolidayPaid, 
(SELECT ROUND(SUM(specialHolidayPaid), 2) FROM attendance Where salaryId = s.id) AS specialHolidayPaid, 
(SELECT ROUND(SUM(workingDayOffPaid), 2) FROM attendance Where salaryId = s.id) AS workingDayOffPaid, b.name AS branch, tn.name AS tradeName, cn.name AS corporateName, b.address AS address, s.payrollDate AS payrollDate, s.attendancePeriodFrom AS startDate, 
s.attendancePeriodTo AS cutOffDate
FROM salary s INNER JOIN employee e ON s.employeeId = e.employeeId INNER JOIN branch b ON e.branchId = b.id INNER JOIN trade_name tn ON b.tradeId = tn.id INNER JOIN corporate_name cn ON 
tn.corporateId = cn.id WHERE e.branchId = $P{BRANCH_ID} AND s.payrollDate = $P{PAYROLL_DATE} ORDER BY e.employmentWageEntry, e.lastname ASC]]>
	</queryString>
